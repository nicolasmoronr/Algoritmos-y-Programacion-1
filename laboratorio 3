bool VeriEsPrimo (int n)
{
	int i ;
	bool k= true;
	for (i=2;i<n;i++)
	{
		if (n%i == 0)
		{
			k= false;
		}
	}
	return (k);
}

int sumaElemtosPrimos (int vec[] ,int n)
{
	int i , s=0:
	for (i=0;i<n;i++)
	{
		if (VeriEsPrimo(vec[i]))
		{
			s=s+vec[i];
		}
	}
	return (s);
}

void cargarPtimos( int vec [] , int n)
{
	int i;
	for(i=0;i<n;i++)
	{
		vec[i] = obtNesimoPrimo(n=i);
	}
}
int obtNesimoPrimo (int n)
{
	int cp=0,c=0;
	while (cp<n)
	{
		c++;
		if(veriEsPrimo (c) == true)
		{
			c++;
		}
	}
	return (c);
}

void eliminarPrimos(int vec[], int &n)
{
	int i , k ;
	for (i=0;i<n;i++)
	{
		if(veriEsPrimo(vec[i]))
		{
			for (k=i;k<n-1;k++)
			{
				vec[k] = vec[k+1];
			}
			n--;
			i--;
		}
	}
}

